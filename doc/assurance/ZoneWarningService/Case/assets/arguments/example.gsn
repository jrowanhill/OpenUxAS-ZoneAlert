        <g
            id="__gsn__13ee712eae9"
            name="Intermediate Goal">
          An intermediate claim that supports the top-level safety claim

          <j
              id="__gsn__13ee712eae3"
              name="Justification">
            Additional explanation of the logic of the claim
          </j>

          <s
              id="__gsn__13ee712eae8"
              name="Intermediate Strategy">
